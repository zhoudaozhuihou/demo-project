    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "axios": "^1.4.0",
    "i18next": "^23.8.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
